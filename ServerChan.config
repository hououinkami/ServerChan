#
# 编译固件设置
#
# 硬件版本
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_DEVICE_generic=y
# 编译UEFI固件
CONFIG_EFI_IMAGES=n
# 取消编译VMware镜像以及镜像填充
# CONFIG_TARGET_IMAGES_PAD is not set
# CONFIG_VMDK_IMAGES is not set
# 固件压缩
CONFIG_TARGET_IMAGES_GZIP=y
# 固件分区大小（默认：16，160）
# CONFIG_TARGET_KERNEL_PARTSIZE=16
# CONFIG_TARGET_ROOTFS_PARTSIZE=160
# 固件语言
CONFIG_LUCI_LANG_ja=n

#
# IPv6
#
# 关闭IPv6
# CONFIG_KERNEL_IPV6=n
# CONFIG_KERNEL_IPV6_MULTIPLE_TABLES=n
# CONFIG_KERNEL_IPV6_SUBTREES=n
# CONFIG_KERNEL_IPV6_MROUTE=n
# CONFIG_IPV6=n
# IPv6支持
# CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
# CONFIG_PACKAGE_ipv6helper=y

#
# 文件系统支持
#


#
# LuCI插件
#
# 安装Lean代码库中的插件
# > 访问时间控制
CONFIG_PACKAGE_luci-app-accesscontrol=n
# > aria2下载
CONFIG_PACKAGE_luci-app-aria2=n
# > IP/MAC绑定
CONFIG_PACKAGE_luci-app-arpbind=n
# > 自动重启
CONFIG_PACKAGE_luci-app-autoreboot=n
# > 动态DNS
CONFIG_PACKAGE_luci-app-ddns=n
# > Docker
CONFIG_PACKAGE_luci-app-docker=n
# > 磁盘管理大师
CONFIG_PACKAGE_luci-app-diskman=n
# > 文件传输
CONFIG_PACKAGE_luci-app-filetransfer=n
# > 防火墙
CONFIG_PACKAGE_luci-app-firewall=n
# > 访客网络
# CONFIG_PACKAGE_luci-app-guest-wifi=n
# > 网络带宽监视器
CONFIG_PACKAGE_luci-app-nlbwmon=n
# > luci-app-nlbwmon正常工作所需（依赖于kmod-iptunnel, kmod-gre, pptpd）
CONFIG_PACKAGE_luci-app-pptp-server=n
# > 释放内存
CONFIG_PACKAGE_luci-app-ramfree=n
# > Turbo ACC网络加速（与luci-app-flowoffload二选一）
CONFIG_PACKAGE_luci-app-sfe=n
# > 流量智能队列管理（QOS）
CONFIG_PACKAGE_luci-app-sqm=n
# > 网页终端命令行
CONFIG_PACKAGE_luci-app-ttyd=n
# > Web管理页面设置
CONFIG_PACKAGE_luci-app-webadmin=n
# > 实时流量监测（依赖于luci-app-nlbwmon）
CONFIG_PACKAGE_luci-app-wrtbwmon=n
# > 远程SSH访问
CONFIG_PACKAGE_openssh-sftp-server=n
# > BaiduPCS-Web
CONFIG_PACKAGE_baidupcs-web=n

# 安装自定义添加插件
# > ServerChan
CONFIG_PACKAGE_luci-app-serverchan=m

# 依赖
CONFIG_PACKAGE_curl=n
CONFIG_PACKAGE_htop=n
CONFIG_PACKAGE_nano=n
CONFIG_PACKAGE_screen=n
CONFIG_PACKAGE_tree=n
CONFIG_PACKAGE_vim-fuller=n
CONFIG_PACKAGE_wget=n

# 禁止编译默认安装
# CONFIG_PACKAGE_luci-app-adbyby-plus is not set
# CONFIG_PACKAGE_luci-app-airplay2 is not set
